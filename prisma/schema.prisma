generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model Contact {
  id        String   @id @default(cuid())
  firstName String
  lastName  String
  phone     String
  email     String 
  createdAt DateTime @default(now())

  @@unique([email, phone])
}
