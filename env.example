# Notion API Configuration
# Copy this file to .env.local and fill in your actual values

# Your Notion integration token (get from https://www.notion.so/my-integrations)
NOTION_TOKEN=your_notion_integration_token_here

# Your main Notion database ID (extract from database URL)
NOTION_DATABASE_ID=your_database_id_here

# Your test Notion database ID (can be the same as main for development)
NOTION_TEST_DATABASE_ID=your_test_database_id_here

# Optional: Database URL for Prisma (if using local database)
# DATABASE_URL="postgresql://username:password@localhost:5432/database_name"
